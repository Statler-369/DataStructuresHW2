@{
    ViewBag.Title = "Display";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Display</h2>
<p>
    <a href="@Url.Action("Index", "Stack")" class="w3-bar-item w3-button">Back to Stack Page</a>
</p>
@{
    string output = "";
    foreach (string newEntry in ViewBag.MyStack)
    {
        output += newEntry + "<br>";
    }
}
@Html.Raw((string)output)